    "compile": "aragon contracts compile",
    "coverage": "SOLIDITY_COVERAGE=true npm run test",
    "lint": "solium --dir ./contracts",
    "start:app": "npm run sync-assets && npm run build:script -- --no-minify && parcel serve app/index.html --port 8001 --out-dir build/app --open",
    "build": "npm run sync-assets && npm run build:app && npm run build:script",
    "build:app": "parcel build app/index.html --out-dir build/app --public-url '.' --no-cache",
    "build:script": "parcel build app/script.js --out-dir build/app  --no-cache",
    "lint:app": "standard app/**/*.js",
    "sync-assets": "copy-aragon-ui-assets -n assets ./build/app",
    "publish": "aragon apm publish"
  },
