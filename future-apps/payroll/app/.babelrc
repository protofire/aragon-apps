{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime",
    "babel-plugin-styled-components"
  ],
  "env": {
    "production": {
      "plugins": [
        ["babel-plugin-react-remove-properties", {
          "properties": ["data-testid"],
        }]
      ]
    }
  }
}
